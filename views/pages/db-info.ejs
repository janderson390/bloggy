<!DOCTYPE html>
<html lan="en">
	<head>
		<meta charset="UTF-8">
		<title>DB Info</title>
	</head>
	<body>
		<h1>Database Tables</h1>
		<ul>
			<% tables.forEach(function(r) { %>
				<li><%= r.table %>: <%= r.column %> (<%= r.type %>)</li>
			<% }); %>
		</ul>
		<h1>Posts</h1>
		<ul>
			<% posts.forEach(function(r) { %>
				<li><%= r.title %> <br> <%= r.body %></li><br>
			<% }); %>
		</ul>
	</body>
</html>